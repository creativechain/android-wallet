<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FontTextView">
        <attr name="font" format="enum">
            <enum name="roboto_light" value="0" />
            <enum name="roboto_bold" value="1" />
            <enum name="roboto_regular" value="2" />
            <enum name="raleway_bold" value="3" />
            <enum name="olivier" value="4" />
            <enum name="material_icon" value="5" />
        </attr>

        <attr name="text_type" format="enum">
            <enum name="html" value="1" />
            <enum name="plain" value="0" />
        </attr>
    </declare-styleable>
</resources>